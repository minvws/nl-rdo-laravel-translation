{
  "name": "minvws/laravel-translation-check",
  "description": "Laravel component for checking translations",
  "keywords": [
    "laravel",
    "translations"
  ],
  "type": "library",
  "license": "EUPL",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.22",
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0",
    "squizlabs/php_codesniffer": "^3.10",
    "mockery/mockery": "^1.6",
    "larastan/larastan": "^2.10 || ^3.0",
    "mikey179/vfsstream": "^1.6.12"
  },
  "autoload": {
    "psr-4": {
      "MinVWS\\Laravel\\Translation\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MinVWS\\Laravel\\Translation\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MinVWS\\Laravel\\Translation\\TranslationServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
        "vendor/bin/phpcs",
        "vendor/bin/phpunit",
        "vendor/bin/phpstan analyse",
        "vendor/bin/psalm"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
